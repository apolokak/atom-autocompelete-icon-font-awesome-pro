{
  "name": "autocomplete-font-awesome-pro",
  "main": "./lib/main",
  "version": "0.1.8",
  "description": "Font Awesome Free or Pro autocomplete and icon previews for Atom.",
  "keywords": [
    "autocomplete",
    "provider",
    "font",
    "awesome",
    "fontawesome",
    "icons",
    "preview"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/apolokak/atom-autocomplete-icon-font-awesome-pro.git"
  },
  "author": "Micko Rj <opensource@apolokak.com> (https://apolokak.com)",
  "license": "MIT",
  "engines": {
    "atom": ">=1.15.0 <2.0.0"
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "getProvider"
      }
    }
  },
  "configSchema": {
    "version": {
      "title": "Font Awesome Version",
      "description": "Which version of Font Awesome Pro to use for suggestions.",
      "type": "string",
      "default": "5.9.0",
      "enum": [
        {
          "value": "4.7.0",
          "description": "Version 4.7.0"
        },
        {
          "value": "5.9.0",
          "description": "Version 5.9.0"
        }
      ]
    }
  }
}
